{
  "name": "dweomery",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "webpack -p --progress --hide-modules  --colors --config ./webpack.config.js",
    "serve": "webpack-dev-server --mode development --open --inline --hot --content-base dist/ --config ./webpack.config.js",
    "proto": "pbjs -t static-module -w es6 -o src/proto.js src/messages.proto",
    "stats": "webpack --profile --json > dist/compilation-stats.json",
    "build-measure1e": "webpack --mode development --env.PAGE_MEASURE=1E --progress --hide-modules  --colors --config ./webpack.config.js",
    "build-measure2e": "webpack --mode development --env.PAGE_MEASURE=2E --progress --hide-modules  --colors --config ./webpack.config.js",
    "measure1e": "node ./src/measure-fetch.js src/assets/spells.json",
    "measure2e": "node ./src/measure-fetch.js src/assets/spells2e.json",
    "download1e": "curl -o converter/spell_full.xlsx 'https://docs.google.com/spreadsheets/d/1cuwb3QSvWDD7GG5McdvyyRBpqycYuKMRsXgyrvxvLFI/export#gid=459662317&format=xlsx'",
    "download2e": "curl -o converter/spells.yaml 'https://gitlab.com/jrmiller82/pathfinder-2-sqlite/-/raw/master/data/yaml/spells.yaml'",
    "spells1e": "cd converter && ./convert_spell_full.py > ../src/assets/spells.json",
    "spells2e": "cd converter && ./convert_spells_yaml.js | jq '.' > ../src/assets/spells2e.json"
  },
  "dependencies": {
    "terser": "^4.6.3",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/runtime": "^7.8.4",
    "@fortawesome/fontawesome-free": "^5.12.1",
    "babel-loader": "^8.0.6",
    "bootstrap": "^4.4.1",
    "compression-webpack-plugin": "^3.1.0",
    "css-loader": "^3.4.2",
    "express": "^4.17.1",
    "favicons-webpack-plugin": "^2.1.0",
    "file-loader": "^4.3.0",
    "geckodriver": "^1.19.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^7.0.0",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "protobufjs": "~6.8.8",
    "sass-loader": "^8.0.2",
    "selenium-webdriver": "^4.0.0-alpha.5",
    "style-loader": "^1.1.3",
    "vue-loader": "^15.8.3",
    "vue-server-renderer": "^2.6.11",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.3",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-subresource-integrity": "^1.4.0"
  }
}
