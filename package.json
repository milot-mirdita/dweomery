{
  "name": "spell-cards",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "webpack -p --progress --hide-modules  --colors --config ./webpack.config.js",
    "serve": "webpack-dev-server --mode development --open --inline --hot --content-base dist/ --config ./webpack.config.js",
    "measure": "webpack --mode development --env.PAGE_MEASURE --progress --hide-modules  --colors --config ./webpack.config.js",
    "measure-serve": "webpack-dev-server --mode development --env.PAGE_MEASURE --open --inline --hot --content-base dist/ --config ./webpack.config.js",
    "proto": "pbjs -t static-module -w es6 -o src/proto.js src/messages.proto",
    "stats": "webpack --profile --json > dist/compilation-stats.json",
    "spells": "cd converter && ./convert_spell_full.py > ../src/assets/spells.json"
  },
  "dependencies": {
    "terser": "^4.2.1",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/runtime": "^7.5.5",
    "@fortawesome/fontawesome-free": "^5.10.2",
    "babel-loader": "^8.0.6",
    "bootstrap": "^4.3.1",
    "compression-webpack-plugin": "^3.0.0",
    "css-loader": "^3.2.0",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "protobufjs": "~6.8.8",
    "sass-loader": "^7.3.1",
    "vue-loader": "^15.7.1",
    "vue-server-renderer": "^2.6.10",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.7",
    "webpack-dev-server": "^3.8.0",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-subresource-integrity": "^1.3.2"
  }
}
